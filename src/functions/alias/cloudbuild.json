{
  "steps": [
    {
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
      "args": [
        "gcloud",
        "functions",
        "deploy",
        "alias",
        "--region=europe-west1",
        "--entry-point=alias",
        "--source=./index.js",
        "--trigger-http",
        "--runtime=nodejs16",
        "--allow-unauthenticated"
      ]
    }
  ],
  "options": {
    "logging": "CLOUD_LOGGING_ONLY"
  }
}
