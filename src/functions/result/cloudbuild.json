{
  "steps": [
    {
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
      "entrypoint": "bash",
      "args": [
        "gcloud",
        "functions",
        "deploy",
        "result",
        "--region=europe-west1",
        "--entry-point=result",
        "--source=src/functions/result",
        "--trigger-http",
        "--runtime=nodejs16",
        "--allow-unauthenticated",
        "--set-env-vars",
        "_ALIAS_URL=$_ALIAS_URL"
      ],
      "secretEnv": ["_ALIAS_URL"]
    }
  ],
  "availableSecrets": {
    "secretManager": [
      {
        "versionName": "projects/883873320046/secrets/_ALIAS_URL/versions/1",
        "env": "_ALIAS_URL"
      }
    ]
  },
  "options": {
    "logging": "CLOUD_LOGGING_ONLY"
  }
}
