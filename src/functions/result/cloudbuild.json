{
  "steps": [
    {
      "name": "gcr.io/cloud-builders/gcloud",
      "entrypoint": "bash",
      "args": [
        "-c",
        "gcloud",
        "functions",
        "deploy",
        "result",
        "--region=europe-west1",
        "--entry-point=result",
        "--source=src/functions/result",
        "--trigger-http",
        "--runtime=nodejs16",
        "--allow-unauthenticated",
        "--set-secrets",
        "_ALIAS_URL=projects/883873320046/secrets/_ALIAS_URL:latest"
      ]
    }
  ],
  "options": {
    "logging": "CLOUD_LOGGING_ONLY"
  }
}
